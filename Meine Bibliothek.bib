	journaltitle = {Nature Chemistry},
	shortjournal = {Nat Chem},
	author = {Auwärter, Willi and Écija, David and Klappenberger, Florian and Barth, Johannes V.},
	urldate = {2015-11-12},
	date = {2015-02},
	langid = {english},
	keywords = {Electronic properties and materials, Surface assembly},
	file = {Full Text PDF:C\:\\Users\\pandigo\\Zotero\\storage\\N5MEQZU6\\Auwärter et al. - 2015 - Porphyrins at interfaces.pdf:application/pdf;Snapshot:C\:\\Users\\pandigo\\Zotero\\storage\\ATVDMCWV\\nchem.2159.html:text/html}
}

