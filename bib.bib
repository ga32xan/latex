@article{przychodzen_supramolecular_2006,
	title = {Supramolecular coordination networks based on octacyanometalates: From structure to function},
	volume = {250},
	issn = {0010-8545},
	url = {http://www.sciencedirect.com/science/article/pii/S0010854506000348},
	doi = {10.1016/j.ccr.2006.01.026},
