@article{liu_dislocations_2012-1,
	title = {Dislocations and Grain Boundaries in Two-Dimensional Boron Nitride},
	volume = {6},
	url = {http://pubs.acs.org/doi/abs/10.1021/nn302099q},
	doi = {10.1021/nn302099q},
	abstract = {A new dislocation structure—square-octagon pair (4{\textbar}8) is discovered in two-dimensional boron nitride (h-{BN}), via first-principles calculations. It has lower energy than corresponding pentagon–heptagon pairs (5{\textbar}7), which contain unfavorable homoelemental bonds. On the basis of the structures of dislocations, grain boundaries ({GB}) in {BN} are investigated. Depending on the tilt angle of grains, {GB} can be either polar (B-rich or N-rich), constituted by 5{\textbar}7s, or unpolar, composed of 4{\textbar}8s. The polar {GBs} carry net charges, positive at B-rich and negative at N-rich ones. In contrast to {GBs} in graphene which generally impede the electronic transport, polar {GBs} have a smaller bandgap compared to perfect {BN}, which may suggest interesting electronic and optical applications.},
	pages = {7053--7058},
	number = {8},
	journaltitle = {{ACS} Nano},
	author = {Liu, Yuanyue and Zou, Xiaolong and Yakobson, Boris I.},
	date = {2012}
}

