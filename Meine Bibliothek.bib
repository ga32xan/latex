	number = {12},
	journaltitle = {Chemistry of Materials},
	shortjournal = {Chem. Mater.},
	author = {Koepke, Justin C. and Wood, Joshua D. and Chen, Yaofeng and Schmucker, Scott W. and Liu, Ximeng and Chang, Noel N. and Nienhaus, Lea and Do, Jae Won and Carrion, Enrique A. and Hewaparakrama, Jayan and Rangarajan, Aniruddh and Datye, Isha and Mehta, Rushabh and Haasch, Richard T. and Gruebele, Martin and Girolami, Gregory S. and Pop, Eric and Lyding, Joseph W.},
