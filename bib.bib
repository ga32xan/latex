	shortjournal = {Phys. Rev. B},
	author = {Zhang, H. G. and Sun, J. T. and Low, T. and Zhang, L. Z. and Pan, Y. and Liu, Q. and Mao, J. H. and Zhou, H. T. and Guo, H. M. and Du, S. X. and Guinea, F. and Gao, H.-J.},
