	url = {http://dx.doi.org/10.1021/acs.nanolett.5b01852},
	doi = {10.1021/acs.nanolett.5b01852},
	abstract = {Grain boundaries ({GBs}) of hexagonal boron nitride (h-{BN}) grown on Cu(111) were investigated by scanning tunneling microscopy/spectroscopy ({STM}/{STS}). The first experimental evidence of the {GBs} composed of square-octagon pairs (4{\textbar}8 {GBs}) was given, together with those containing pentagon-heptagon pairs (5{\textbar}7 {GBs}). Two types of {GBs} were found to exhibit significantly different electronic properties, where the band gap of the 5{\textbar}7 {GB} was dramatically decreased as compared with that of the 4{\textbar}8 {GB}, consistent with our obtained result from density functional theory ({DFT}) calculations. Moreover, the present work may provide a possibility of tuning the inert electronic property of h-{BN} via grain boundary engineering.},
	pages = {5804--5810},
	number = {9},
	journaltitle = {Nano Letters},
	shortjournal = {Nano Lett.},
	author = {Li, Qiucheng and Zou, Xiaolong and Liu, Mengxi and Sun, Jingyu and Gao, Yabo and Qi, Yue and Zhou, Xiebo and Yakobson, Boris I. and Zhang, Yanfeng and Liu, Zhongfan},
