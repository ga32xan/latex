	keywords = {Adenosine Diphosphate, Animals, Binding Sites, Cattle, Glutamate Dehydrogenase, Glutamates, Guanosine Triphosphate, Kinetics, Liver, {NAD}, {NADP}, Protein Binding, Spectrophotometry, Ultraviolet, Time Factors}
