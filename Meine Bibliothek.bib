	abstract = {The transmission coefficient D(V) and the tunnel current I(V) for a trapezoidal potential barrier is calculated as a function of applied voltage V. In earlier works the {WKB} approximation was used evaluating the transmission coefficient and therefore only the barrier between the classical turning points was considered. The {WKB} transmission coefficient of a trapezoidal potential barrier is a monotonically decreasing function of applied voltage. In the present paper the exact treatment of the whole barrier shows that the transmission coefficient D(V) can be a function with superposed periodic oscillations. These oscillations are caused by partial reflection and interference of electron waves in the part of the trapezoidal barrier, where the potential energy is less than the kinetic energy of the electron. Also the calculated tunnel current I(V) can have a noticeable superposed oscillation. For observing this oscillation in metal-insulator-metal films the conditions are large electron mean free path (probable some hundred {AE}) in the dielectric layer and specular reflection from the barrier boundaries.},
	pages = {949--957},
	number = {10},
	journaltitle = {Solid-State Electronics},
	shortjournal = {Solid-State Electronics},
	author = {Gundlach, K. H.},
	urldate = {2015-07-22},
	date = {1966-10}
}

@article{ng_formation_2008,
	title = {Formation and temperature evolution of Au nanoparticles supported on the h-{BN} nanomesh},
	volume = {602},
	issn = {0039-6028},
	url = {http://www.sciencedirect.com/science/article/pii/S0039602808000630},
	doi = {10.1016/j.susc.2008.01.028},
	abstract = {Sub-monolayers of gold have been grown at room temperature on the hexagonal boron nitride (h-{BN}) nanomesh formed on Rh(1\&\#xa0;1\&\#xa0;1), and studied systematically with core level and angle-resolved valence band photoelectron spectroscopy. The results are compared with those for the Au/Rh(1\&\#xa0;1\&\#xa0;1) and Au/h-{BN}/Pt(1\&\#xa0;1\&\#xa0;1) interfaces. It has been found that on clean Rh(1\&\#xa0;1\&\#xa0;1) substrate gold starts to grow two-dimensionally (2D), while in the presence of a h-{BN} interlayer it forms islands from the very beginning. In the case of flat h-{BN} monolayer (on Pt) these islands are essentially three-dimensional (3D) and irregular in size. In contrast, on the h-{BN} nanomesh (on Rh) gold grows initially as regular islands (predominantly 2D), probably filling the pores of the nanomesh. In addition, the evolution of the Au islands in h-{BN}/Rh(1\&\#xa0;1\&\#xa0;1) upon annealing has been investigated with core level photoemission and X-ray absorption. The annealing at temperatures between {RT} and 300\&\#xa0;°C does not affect the 2D character of Au islands, while further increase in temperature results in formation of 3D islands. At higher temperatures (above 500\&\#xa0;°C), the gold atoms diffuse through h-{BN} into the Rh substrate and/or desorb from the surface. However, they do not influence the chemical bond between rhodium and h-{BN} at any stage of annealing: the nanomesh remains intact.},
	pages = {1250 -- 1255},
	number = {6},
	journaltitle = {Surface Science},
	author = {Ng, M. L. and Preobrajenski, A. B. and Vinogradov, A. S. and Mårtensson, N.},
	date = {2008},
	keywords = {Au, nanoparticles}
}

