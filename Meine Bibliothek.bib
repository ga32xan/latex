@article{joshi_control_2014-3,
	title = {Control of Molecular Organization and Energy Level Alignment by an Electronically Nanopatterned Boron Nitride Template},
	volume = {8},
	issn = {1936-0851},
	url = {http://dx.doi.org/10.1021/nn406024m},
	doi = {10.1021/nn406024m},
	abstract = {Suitable templates to steer the formation of nanostructure arrays on surfaces are indispensable in nanoscience. Recently, atomically thin sp2-bonded layers such as graphene or boron nitride ({BN}) grown on metal supports have attracted considerable interest due to their potential geometric corrugation guiding the positioning of atoms, metallic clusters or molecules. Here, we demonstrate three specific functions of a geometrically smooth, but electronically corrugated, sp2/metal interface, namely, {BN}/Cu(111), qualifying it as a unique nanoscale template. As functional adsorbates we employed free-base porphine (2H?P), a prototype tetrapyrrole compound, and tetracyanoquinodimethane ({TCNQ}), a well-known electron acceptor. (i) The electronic moirons of the {BN}/Cu(111) interface trap both 2H?P and {TCNQ}, steering self-organized growth of arrays with extended molecular assemblies. (ii) We report an effective decoupling of the trapped molecules from the underlying metal support by the {BN}, which allows for a direct visualization of frontier orbitals by scanning tunneling microscopy ({STM}). (iii) The lateral molecular positioning in the superstructured surface determines the energetic level alignment; i.e., the energy of the frontier orbitals, and the electronic gap are tunable.},
	pages = {430--442},
	number = {1},
	journaltitle = {{ACS} Nano},
	shortjournal = {{ACS} Nano},
	author = {Joshi, Sushobhan and Bischoff, Felix and Koitz, Ralph and Ecija, David and Seufert, Knud and Seitsonen, Ari Paavo and Hutter, Jürg and Diller, Katharina and Urgel, José. I. and Sachdev, Hermann and Barth, Johannes V. and Auwärter, Willi},
	urldate = {2014-10-28},
	date = {2014-01-28},
	file = {ACS Full Text PDF w/ Links:C\:\\Users\\pandigo\\Zotero\\storage\\RDCQ5IND\\Joshi et al. - 2014 - Control of Molecular Organization and Energy Level.pdf:application/pdf;ACS Full Text Snapshot:C\:\\Users\\pandigo\\Zotero\\storage\\EX9MBK5E\\nn406024m.html:text/html}
}

@article{guo_controllable_2012,
	title = {Controllable growth of triangular hexagonal boron nitride domains on copper foils by an improved low-pressure chemical vapor deposition method},
	volume = {23},
	issn = {0957-4484},
	url = {http://iopscience.iop.org/0957-4484/23/41/415605},
	doi = {10.1088/0957-4484/23/41/415605},
	abstract = {We demonstrate an improved low-pressure chemical vapor deposition method to fabricate hexagonal boron nitride (h-{BN}) domains of a few layers (one–four layers) from ammonia borane by adding a small quartz tube to stabilize the gas flow over the copper substrate and reducing the growing rate of h-{BN}. The h-{BN} grows freely and spontaneously to form triangular domains on the Cu (100) plane. The triangular domains are prone to be parallel to each other on the copper substrate. The h-{BN} domains grow by extending in the normal direction of the triangle and form a large thin film by joining together. Both the size and coverage rate on Cu foils are well controlled by tuning the amount of ammonia borane.},
	pages = {415605},
	number = {41},
