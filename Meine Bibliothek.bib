	file = {Full Text PDF:C\:\\Users\\pandigo\\Zotero\\storage\\QT6KJBK9\\Gutiérrez et al. - 2016 - Imaging chiral symmetry breaking from Kekule bond .pdf:application/pdf;Snapshot:C\:\\Users\\pandigo\\Zotero\\storage\\XD8TRGGI\\nphys3776.html:text/html}
